#include "timers.h"

alt_alarm timer;


void test_timer(){
	void* context = 0;


	IOWR_ALTERA_AVALON_PIO_DATA(LEDS_GREEN_BASE, 0b10);
	printf("timer started");
	alt_alarm_start(&timer, LED_TIMEOUT, APLEDTimerISR, context);

}

