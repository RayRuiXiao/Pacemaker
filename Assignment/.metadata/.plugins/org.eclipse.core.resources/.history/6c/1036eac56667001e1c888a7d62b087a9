#include "uart.h"

void check_uart(){
	if (read(uart,byte,1) > 0){
		if (*byte == 'A'){
			printf("ASense UART\n");
		}
		else if (*byte == 'V'){
			printf("VSense UART\n");
		}
		else{
			// set v and a to false
		}
	}
	else{
		// set v and a to false
	}
}

